{
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "eslint.validate": [
        "vue",
        "html",
        "javascript",
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "deno.enable": false,
    "i18n-ally.localesPaths": [
        "locale"
    ]
}